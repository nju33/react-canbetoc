{
  "name": "@nju33/react-canbetoc",
  "version": "0.0.18",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/nju33/react-canbetoc.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "nju33 <nju33.ki@gmail.com>",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.m.js",
  "esmodule": "dist/index.modern.js",
  "source": "src/index.ts",
  "files": [
    "*/*.map",
    "**/*.css",
    "**/*.js",
    "**/*.d.ts"
  ],
  "license": "MIT",
  "scripts": {
    "patch": "yarn version --patch",
    "update": "yarn version --minor",
    "upgrade": "yarn version --upgrade",
    "dev": "start-storybook --port 33817 --static-dir ./",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "test": "jest --config jest.config.js",
    "ci:build": "[ \"$CI\" = 'true' ] && yarn microbundle --tsconfig tsconfig.build.json --jsx 'React.createElement' --format modern,es,cjs --no-compress || :",
    "ci:copy-css": "[ \"$CI\" = 'true' ] && cp src/strategies/collapse/*.css strategies/collapse/ && cp src/strategies/expansion/*.css strategies/expansion/ || :",
    "ci:gen-src-docs": "typedoc --plugin typedoc-plugin-markdown --out docs src",
    "ci:test": "jest --config jest.config.js --collectCoverage"
  },
  "peerDependencies": {
    "fp-ts": "^2.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@types/throttle-debounce": "^2.1.0",
